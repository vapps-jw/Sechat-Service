{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "System": "Warning",
      "Microsoft.*": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "sechat"
    }
  },
  "ConnectionStrings": {
    "Master": "[secret]"
  },
  "CorsSettings": {
    "WebAppUrl": "[secret]",
    "ApiUrl": "[secret]"
  },
  "CookieSettings": {
    "AuthCookieDomain": "[secret]",
    "AuthCookieName": "[secret]"
  },
  "EmailSenderSettings": {
    "ApiKey": "[secret]",
    "From": "[secret]",
    "ResetPasswordTemplate": "[secret]",
    "ConfirmEmailTemplate": "[secret]"
  },
  "VapidKeys": {
    "PublicKey": "[secret]",
    "PrivateKey": "[secret]"
  }
}
